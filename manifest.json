{
    "manifest_version":3,
    "name":"Smart History Filter",
    "description":"Filters browsing history by domain or full url based on exceptions.",
    "version":"1.0",
    "permissions":[
        "history",
        "storage"
    ],
    "background":{
        "service_worker":"background.js"
    },
    "action":{
        "default_popup":"popup.html",
        "default_icon":{
            "16":"icons/icon16.png",
            "32":"icons/icon32.png"
        },
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png"
        
        }
    }
}